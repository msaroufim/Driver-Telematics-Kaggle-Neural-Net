   "source": [
    "import mmh3\n",
    "\n",
    "def hash(key,num_hashes,num_bits):\n",
    "    h1,h2 = mmh3.hash64(key)\n",
    "    for i in xrange(num_hashes):\n",
    "        yield (h1 + i * h2) % num_bits"
   ]
